EJERCICIO 1
// Crea un programa en TypeScript donde se declaren variables para almacenar tu
// nombre, edad y si eres estudiante, utilizando los tipos de datos correctos.
// Luego, imprime esos valores en la consola.

let nombre: string = Nina";
let edad: number = 46;
let eresEstudiante: boolean = true;
console.log('Nombre: ', nombre);
console.log('Edad: ', edad);
console.log('Eres estudiante ', eresEstudiante);

EJERCICIO 2

let variableAny: any = 48;
console.log(variableAny);
variableAny = "Variable any en string";
console.log(variableAny);

