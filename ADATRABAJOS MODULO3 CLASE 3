EJERCICIO 1    1.main.ts    //Exportacion, modularizacion basica
import {add, subtract} from './1.mathUtils';

let result: number;

result = add(5,10);
console.log(`El resultado de la suma es: ${result}`);

result = subtract(10,5);
console.log(`El resultado de la resta es: ${result}`

1. mathUtils.ts

// Actividad 1:

Modularizaci√≥n BASICA

export function add(a: number, b:number) : number {
    return a + b;
}

